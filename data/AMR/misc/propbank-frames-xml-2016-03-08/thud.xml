        <alias framenet="Motion_noise Impact Cause_impact" pos="v" verbnet="">thud</alias>
      </aliases>
      <note>THUD-V NOTES: Added by Kev. Member of Vncls bump-18.4-1, sound_emission-43.2. (from thud.01-v)</note>
      <roles>
        <role descr="thing thudding" f="PPT" n="0">
          <vnrole vncls="18.4-1" vntheta="Theme"/>
          <vnrole vncls="43.2" vntheta="Theme"/>
        </role>
        <role descr="thing hit (NOT Location)" f="LOC" n="1">
          <vnrole vncls="18.4-1" vntheta="Location"/>
          <vnrole vncls="43.2" vntheta="Location"/>
        </role>
      </roles>
      <example name="When the levee breaks" src="" type="">
        <text>Waves thudded against the seawall.</text>
        <arg f="" n="0">waves</arg>
        <rel f="">thudded</rel>
        <arg f="" n="1">against the seawall</arg>
      </example>
      <example name="Nervousness" src="" type="">
        <text>Josephine's heart thudded inside her chest.</text>
        <arg f="" n="0">Josephine's heart</arg>
        <rel f="">thudded</rel>
        <arg f="loc" n="m">inside her chest</arg>
      </example>
    </roleset>
