    <roleset id="question.02" name="out-of-the-question">
      <aliases>
        <alias framenet="" pos="n" verbnet="">question</alias>
      </aliases>
      <roles>
        <role descr="thing that is out of the question" f="ppt" n="1"/>
        <role descr="for whom/what" f="gol" n="2"/>
      </roles>
      <example name="arg1" src="" type="">
        <text>Sometimes repairs are out of the question .</text>
	<arg f="tmp" n="m">Sometimes</arg>
	<arg f="" n="1">repairs</arg>
        <rel f="">are out of the question</rel>
      </example>
      <example name="arg1 and arg2" src="" type="">
        <text>renewing military aid had been considered out of the question for civilians,</text>
        <arg f="" n="1">renewing military aid</arg>
        <rel f="">out of the question</rel>
        <arg f="" n="2">for civilians</arg>
      </example>
    </roleset>
